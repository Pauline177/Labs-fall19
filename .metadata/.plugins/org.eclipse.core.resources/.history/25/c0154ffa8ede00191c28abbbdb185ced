package labPrinter;

public class PrinterApp {

	public static void main(String[] args) {
		
	LaserPrinter lPrinter = new LaserPrinter(1234567);
	InkjetPrinter iPrinter = new InkjetPrinter(238889);
		
	System.out.println(lPrinter);
	System.out.println(lPrinter.getRemainingToner()+ "%");
	System.out.println();
	
	
	System.out.println(iPrinter);
	System.out.println(iPrinter.getRemainingCartridge() + "%");
	System.out.println();
	
	Printer[] printersList = {lPrinter, iPrinter};
	System.out.println("Array of Printers");
	for (Printer p: printersList) {
		System.out.println(p);
		p.print();
		System.out.println();
	}
	
}
	
}

