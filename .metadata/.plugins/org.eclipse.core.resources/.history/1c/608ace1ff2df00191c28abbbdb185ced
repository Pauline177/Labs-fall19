package module02;

/**
 * @author paulinemakoma
 *
 */
public  class DeskPedal {

	private String color;
	
	public DeskPedal(String c) {
		this.color = c;
	}
	
	public String getColor() {
		return color;
	}
	
	public  void pedal() {
		System.out.println("pedaling..");
	}
	

	public String toString() {
		return color + " " + this.getClass().getSimpleName() ;
	}
}
